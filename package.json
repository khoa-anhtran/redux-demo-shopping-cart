{
  "name": "rtk-cart",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:api1": "json-server --watch mock/db.json --routes mock/routes.json --port 4000 --delay 400",
    "dev:api": "node mock/server.cjs",
    "dev:web": "vite",
